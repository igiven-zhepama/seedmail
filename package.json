{
  "name": "seedmail",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build  && npm run build:pages",
    "build:pages": "tsc -p functions/tsconfig.json ",
    "pages:dev": "wrangler pages dev functions --compatibility-date=2024-11-10",
    "pages:preview": "wrangler pages dev dist --compatibility-date=2024-11-10",
    "preview": "vite preview"
  },
  "dependencies": {
    "dompurify": "^3.2.0",
    "mailparser": "^3.7.1",
    "resend": "^4.0.1-alpha.0",
    "tdesign-vue-next": "^1.10.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/mailparser": "^3.4.5",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "unplugin-auto-import": "^0.18.4",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.8",
    "wrangler": "^3.87.0"
  }
}
